<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="using:ThePhotoStore">

    <Style TargetType="local:ImageLogoControl">
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="local:ImageLogoControl">
                    <StackPanel Orientation="Vertical">
                        <ListBox >
                            <ListBoxItem>
                                <StackPanel  Orientation="Horizontal">
                                    <Image Source="Assets/images/Camera.png" Height="100" Width="120" >
                                        
                                    </Image>
                                
                                
                                </StackPanel>
                            </ListBoxItem>
                        </ListBox>
                    </StackPanel>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>

    
</ResourceDictionary>

